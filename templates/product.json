    "product_faq_nUWLHm": {
      "type": "product-faq",
      "blocks": {
        "faq_mXRMVM": {
          "type": "faq",
          "settings": {
            "question": "1、<strong>What lids and mason jar brands are heiyo products suitable for?</strong>",
            "answer": "heiyo’s vacuum sealers are designed to fit universally to any and every mason jar brand and lid, saving you money by eliminating the need to buy special ones. You can use any mason jars, whether from Ball, Kerr, Bernardin, or any other brand! The same goes for lids - you can use any store-bought lids regardless of the brand."
          }
        },
        "faq_UNJTf6": {
          "type": "faq",
          "settings": {
            "question": "2、<strong>What jar sizes does heiyo’s VP05 fit?</strong>",
            "answer": "The heiyo’s VP05 is designed to fit both regular and wide-mouth jars, providing versatility in preserving your favorite foods."
          }
        },
        "faq_d7mVUc": {
          "type": "faq",
          "settings": {
            "question": "3、<strong>How does the heiyo’s VP05 charge?</strong>",
            "answer": "The VP05 features USB-C charging for quick and versatile power, with a single charge supporting up to 50 seals."
          }
        },
        "faq_kEyTGc": {
          "type": "faq",
          "settings": {
            "question": "4、<strong>What is the battery life of the VP05?</strong>",
            "answer": "A single charge supports up to 50 seals, making it ideal for extended use and a reliable kitchen companion."
          }
        },
        "faq_4twCWY": {
          "type": "faq",
          "settings": {
            "question": "5、<strong>How do I clean and maintain the VP05?</strong>",
            "answer": "Simply wipe the VP05 with a damp cloth, avoid direct contact with water near the charging port, and store it in a dry place."
          }
        }
      },
